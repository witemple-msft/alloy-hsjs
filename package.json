{
  "name": "@typespec/http-server-js",
  "version": "0.5.0",
  "type": "module",
  "homepage": "https://typespec.io",
  "readme": "https://github.com/microsoft/typespec/blob/main/packages/http-client-js/README.md",
  "scripts": {
    "build": "npm run build:helpers && alloy build",
    "build:helpers": "tsx scripts/build-helpers.ts",
    "clean": "rimraf ./dist",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "watch": "alloy build --watch",
    "format": "prettier . --write"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "keywords": [
    "typespec",
    "javascript",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/microsoft/typespec/issues"
  },
  "author": "Microsoft Corporation",
  "license": "MIT",
  "description": "TypeSpec library for emitting Http Client libraries for JavaScript/TypeScript",
  "peerDependencies": {
    "@typespec/compiler": "^1.1.0",
    "@typespec/http": "^1.1.0"
  },
  "dependencies": {
    "@alloy-js/core": "next",
    "@alloy-js/typescript": "next",
    "@typespec/emitter-framework": "latest",
    "prettier": "~3.5.3"
  },
  "devDependencies": {
    "@alloy-js/cli": "next",
    "@alloy-js/rollup-plugin": "^0.1.0",
    "@types/node": "^24.0.3",
    "@types/yargs": "~17.0.33",
    "@typespec/http": "^1.1.0",
    "@vitest/ui": "^3.1.2",
    "change-case": "~5.4.4",
    "concurrently": "^9.1.2",
    "cross-env": "~7.0.3",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "globby": "~14.1.0",
    "inquirer": "^12.5.0",
    "ora": "^8.1.1",
    "p-limit": "^6.2.0",
    "picocolors": "~1.1.1",
    "typescript": "~5.8.2",
    "uri-template": "^2.0.0",
    "vitest": "^3.1.2",
    "yargs": "~17.7.2",
    "temporal-polyfill": "^0.3.0",
    "express": "^5.1.0",
    "@types/express": "^5.0.3"
  }
}
